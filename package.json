{
  "name": "mathina-hub",
  "version": "1.0.0",
  "description": "Mathina Hub App",
  "author": "Seven",
  "private": true,
  "scripts": {
    "dev": "nuxt --config-file src/hub/nuxt.config.js -p=3000",
    "generate": "nuxt generate --config-file src/hub/nuxt.config.js -p=3000",
    "hub:dev": "yarn run dev",
    "hub:generate": "yarn run generate",
    "edu:dev": "nuxt --config-file src/educators/nuxt.config.js -p=3002",
    "edu:generate": "nuxt generate --config-file src/educators/nuxt.config.js -p=3002",
    "app:dev": "nuxt --config-file src/app/nuxt.config.js -p=3001",
    "app:generate": "nuxt generate --config-file src/app/nuxt.config.js && src/app/build-mod.sh",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.1.2",
    "gsap": "^3.5.1",
    "js-yaml": "^3.14.0",
    "js-yaml-loader": "^1.2.2",
    "markdown-it-html5-media": "^0.7.0",
    "markdown-it-table-of-contents": "^0.5.2",
    "markdown-it-vue": "^1.1.6",
    "nuxt": "^2.14.6",
    "nuxt-i18n": "^6.15.1",
    "stylelint-config-standard": "^20.0.0",
    "yaml-loader": "^0.6.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.5.2",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-nuxt": ">=1.0.0",
    "jest": "^26.5.2",
    "postcss-css-variables": "^0.17.0",
    "postcss-hexrgba": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-import-resolver": "^2.0.0",
    "postcss-nested": "4.2.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "stylelint": "^13.7.2",
    "vue-jest": "^3.0.7"
  }
}
